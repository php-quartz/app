{
    "name": "php-quartz/app",
    "type": "project",
    "license": "MIT",
    "description": "Job Time Scheduler App",
    "keywords": ["job", "time", "task", "time scheduler", "quartz", "chrono"],
    "require": {
        "php": "^7.1.3",
        "symfony/console": "^3.3",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^3.3",
        "symfony/yaml": "^3.3",
        "enqueue/psr-queue": "^0.5@dev",
        "enqueue/enqueue": "^0.5@dev",
        "enqueue/amqp-ext": "^0.5@dev",
        "enqueue/enqueue-bundle": "^0.5@dev",
        "makasim/yadm": "@dev",
        "php-quartz/quartz": "@dev",
        "php-quartz/bridge": "@dev",
        "php-quartz/bundle": "@dev"
    },
    "require-dev": {
        "symfony/dotenv": "^3.3",
        "phpunit/phpunit": "~5.5",
        "enqueue/null": "^0.5@dev"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Quartz\\App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Quartz\\App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "make cache-warmup": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BJQZD7YBT9JB0NGH2G58754G",
            "allow-contrib": false
        }
    }
}
